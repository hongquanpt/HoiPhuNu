@{
     var list = (List<Web_Girls.Models.Context.ThamGia>)ViewBag.thamgia;


}
<div class="modal-content">
     <div class="modal-header">
          <h4 class="modal-title">Danh sách tham gia đề tài</h4>
          <button type="button" class="close" data-dismiss="modal">&times;</button>
     </div>
     <div class="modal-body">
          @if (list != null)
          {
               <table class="table table-hover table-bordered text-center">
                    <thead>
                         <tr>
                              <td>Họ tên</td>
                              <td>Đơn vị</td>
                              <td>Vai trò</td>

                         </tr>
                    </thead>
                    <tbody>
                         @foreach (var item in list)
                         {
                         <tr>
                              <td>@item.HoTen</td>
                              <td>@item.DonVi</td>
                              <td>@item.VaiTro</td>
                         </tr>
                         }
                    </tbody>
               </table>
          }
          else
          {
               <h2>Chưa có thông tin</h2>
          }


     </div>
</div>




